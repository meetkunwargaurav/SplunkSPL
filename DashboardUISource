<form version="1.1" theme="light">
  <label>Change_Management - Change Management Automation Dashboard | Population Coverage - MMM DD, YYYY, to Last Update Date (i.e., MMM DD, YYYY)</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="dropdown" token="App_Filter" searchWhenChanged="true">
      <label>Select Application / Infra</label>
      <fieldForLabel>Configuration_Item</fieldForLabel>
      <fieldForValue>Configuration_Item</fieldForValue>
      <search>
        <query>| inputlookup csvfile.csv 
| dedup Configuration_Item</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <choice value="*">All</choice>
      <initialValue>*</initialValue>
      <default>*</default>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Total # of Changes</title>
      <single>
        <search>
          <query>| inputlookup csvfile.csv |search Configuration_Item="$App_Filter$" | stats DC(Task)</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </single>
    </panel>
    <panel>
      <title>Total # of Applications / Infrastructure Devices using SNOW for Change Management</title>
      <single>
        <search>
          <query>| inputlookup csvfile.csv 
| stats DC(Configuration_Item)</query>
          <earliest>0</earliest>
          <latest></latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </single>
