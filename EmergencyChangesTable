| inputlookup csvfile.csv|search Configuration_Item="*" |search Type=Emergency|stats values(Configuration_Item) as Configuration_Item values(Short description) as Short_Description values(Created) as Created_On values(Approval set) as Approved_On values(Closed) as Implemented_On values(Approval history) as "All_ApproverNames_with_Date_of_Approval" values(Assigned to) as Implementor values(Close notes) as Close_Notes by Task|eval Implemented_Post_Approval? = if(Closed < Approval_set, "No", "Yes")|eval Approver_vs_Implementor_SOD_Maintained?=if (like(Approval_history,"%".Assigned_to."%"),"No","Yes")|eval Test_Result=if (like(Approval_history,"%".Assigned_to."%"),"Failed",if(Closed < Approval_set,"Failed","Passed"))
